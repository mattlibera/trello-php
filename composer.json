{
    "name" : "mattlibera/trello-php",
    "description" : "PHP wrapper for Trello API - forked for use in Laravel 5.4",
    "keywords" : ["trello","php","client","wrapper","trello gold","laravel"],
    "homepage" : "http://github.com/mattlibera/trello-php",
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Matt Libera",
            "email" : "matt@mattlibera.com",
            "homepage" : "http://mattlibera.com",
            "role" : "Developer"
        }
    ],
    "require" : {
        "guzzlehttp/guzzle": "^6.0",
        "league/oauth1-client": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.*@dev",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Mattlibera\\Services\\Trello\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mattlibera\\Services\\Trello\\Tests\\": "tests/"
        },
        "classmap": [
            "src/",
            "tests/"
        ]
    }
}
